<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="cn.yingchuang.dao.ApplyNum.ApplyNumMapper">

    <!--修改最大值-->
    <update id="updateMaxNum">
        UPDATE applynum SET applyNum=applyNum+1
    </update>

    <!--查询最大值-->
    <select id="queryMaxNum" resultType="Integer">
        SELECT applyNum FROM applynum
    </select>

</mapper>