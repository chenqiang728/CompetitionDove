<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="cn.yingchuang.dao.Apply.ApplyMapper">

    <!--添加-->
    <insert id="addApply" parameterType="Apply">
        INSERT INTO apply VALUE(NULL,#{informationId},#{raceId},NOW(),#{applyCode})
    </insert>

    <!--查询所有-->
    <select id="queryAllApply" resultType="Apply">
        SELECT * FROM apply
    </select>

</mapper>